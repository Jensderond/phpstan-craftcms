{
	"name": "jensderond/phpstan-craftcms",
	"description": "CraftCMS extension for PHPStan",
	"version": "1.1.2",
	"require": {
		"phpstan/phpstan": "^2.1"
	},
	"require-dev": {
		"craftcms/cms": "~5",
		"laravel/pint": "^1.25"
	},
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Jensderond\\PhpstanCraftcms\\": "src/"
		}
	},
	"authors": [
		{
			"name": "Jens de Rond",
			"email": "jensderond@gmail.com"
		}
	],
	"scripts": {
		"lint": "pint"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
